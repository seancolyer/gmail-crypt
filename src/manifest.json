{
"name":"Mymail-Crypt for Gmail™",
"version":"29",
"manifest_version":2,
"description":"Extension implementing GPG into Webmail (Gmail™) using the OpenPGP.js library. This project is no way affiliated with Google™.",
"icons": {
    "128":"gmail-crypt128.png"
    },
"permissions":[
    "https://mail.google.com/*",
    "http://mail.google.com/*",
    "https://google.com/*",
    "http://google.com/*"
    ],
"background": {
    "scripts":["lib/jquery-2.0.2.min.js", "lib/openpgp.js", "gmail-cryptUtil.js", "gmail-cryptBackground.js"]
    },
"content_scripts": [{
    "matches":["https://mail.google.com/*","http://mail.google.com/*"],
    "css": ["css/bootstrap.css", "css/gmail-crypt.css"],
    "js": ["lib/jquery-2.0.2.min.js", "lib/bootstrap.min.js", "lib/openpgp.js","gmail-cryptUtil.js","gmail-crypt.js"],
    "run_at":"document_idle",
    "all_frames":false
}],
"homepage_url":"http://prometheusx.net",
"options_page":"gmail-cryptOptions.html",
"web_accessible_resources": [
    "images/encrypt_closed.png",
    "images/encrypt_open.png",
    "images/encrypt_sign_closed.png",
    "images/encrypt_sign_open.png",
	"images/sign_open.png",
	"images/sign_closed.png"
    ]
}
